{
  "manifest_version": 2,
  "name": "avanza-sticky",
  "description": "This makes the header stick to the top",
  "version": "1",
  "content_scripts": [
    {
      "js": [
        "fixer.js"
      ],
      "matches": [
        "http://www.avanza.se/*",
        "https://www.avanza.se/*"
      ],
      "run_at": "document_end"
    }
  ]
}
